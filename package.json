{
  "name": "xapps-geo-ts",
  "private": true,
  "scripts": {
    "build": "remix vite:build",
    "build:assets": "NODE_ENV=development vite build --config vite.assets.config.ts && vite build --config vite.assets-markets.config.ts",
    "build:assets:prod": "NODE_ENV=production vite build --config vite.assets.config.ts && vite build --config vite.assets-markets.config.ts",
    "watch:assets": "NODE_ENV=development vite build --config vite.assets.config.ts --watch & NODE_ENV=development vite build --config vite.assets-markets.config.ts --watch",
    "build:all": "npm run build:assets && npm run build",
    "dev": "npm run build:assets && concurrently \"npm run watch:assets\" \"shopify app dev\"",
    "dev:assets": "npm run watch:assets",
    "config:link": "shopify app config link",
    "generate": "shopify app generate",
    "deploy:dev": "npm run build:assets && shopify app deploy -f",
    "deploy": "npm run build:assets:prod && shopify app deploy",
    "config:use": "shopify app config use",
    "env": "shopify app env",
    "start": "remix-serve ./build/server/index.js",
    "docker-start": "npm run setup && npm run start",
    "setup": "prisma generate && prisma migrate deploy",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "shopify": "shopify",
    "prisma": "prisma",
    "graphql-codegen": "graphql-codegen",
    "vite": "vite"
  },
  "type": "module",
  "engines": {
    "node": "^18.20 || ^20.10 || >=21.0.0"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@prisma/client": "^6.2.1",
    "@remix-run/dev": "^2.16.5",
    "@remix-run/fs-routes": "^2.16.5",
    "@remix-run/node": "^2.16.5",
    "@remix-run/react": "^2.16.5",
    "@remix-run/serve": "^2.16.5",
    "@shopify/app-bridge-react": "^4.1.8",
    "@shopify/polaris": "^13.9.5",
    "@shopify/shopify-app-remix": "^3.8.2",
    "@shopify/shopify-app-session-storage-prisma": "^6.0.5",
    "@uiw/codemirror-extensions-basic-setup": "^4.23.10",
    "@uiw/codemirror-extensions-langs": "^4.23.10",
    "@uiw/react-codemirror": "^4.23.10",
    "date-fns": "^4.1.0",
    "fs-backwards-stream": "^1.0.0",
    "got": "^14.4.7",
    "isbot": "^5.1.0",
    "prisma": "^6.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-quill": "^2.0.0",
    "sass": "^1.85.0",
    "uniqid": "^5.4.0",
    "valid-url": "^1.0.9",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "devDependencies": {
    "@remix-run/eslint-config": "^2.16.5",
    "@remix-run/route-config": "^2.16.5",
    "@shopify/api-codegen-preset": "^1.1.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.2.0",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@types/uniqid": "^5.3.4",
    "@types/valid-url": "^1.0.7",
    "concurrently": "^9.2.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^10.0.1",
    "prettier": "^3.2.4",
    "terser": "^5.43.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.3"
  },
  "workspaces": [
    "extensions/*"
  ],
  "trustedDependencies": [
    "@shopify/plugin-cloudflare"
  ],
  "resolutions": {},
  "overrides": {},
  "author": "sanjar"
}
